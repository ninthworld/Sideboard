        <div ng-style="{width: game.cardRatio * game.getMinSize(2) + 'px', height: game.cardRatio * game.getMinSize(2) + 'px', 'background-color': 'var(--'+game.color[1]+'-900)'}">

        </div>
        <div ng-style="{
