                    'margin-top': 0.06 * game.cardRatio * game.getMinSize(2) + 'px'
                }">Graveyard</label>
            </div>
            <div ng-style="{width: game.cardRatio * game.getMinSize(2) + 'px', height: game.cardRatio * game.getMinSize(2) + 'px', 'background-color': 'var(--'+game.color[0]+'-400)'}">
                <label layout="column" class="gc-label" ng-style="{
                    'font-size': 0.12 * game.cardRatio * game.getMinSize(2) + 'px', 
                    'margin-left': 0.32 * game.cardRatio * game.getMinSize(2) + 'px', 
                    'margin-top': 0.06 * game.cardRatio * game.getMinSize(2) + 'px'
                }">Deck</label>
            </div>
            <div ng-style="{width: game.cardRatio * game.getMinSize(2) + 'px', 'background-color': 'var(--'+game.color[0]+'-500)'}" flex>
            
            </div>
        </div>
        <div layout="row" style="background-color: var(--blue-800);" flex>
